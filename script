ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
Frame.BorderColor3 = Color3.fromRGB(24, 24, 24)
Frame.Position = UDim2.new(0.161917105, 0, 0.0676883757, 0)
Frame.Size = UDim2.new(0, 282, 0, 339)
Frame.Active = true
Frame.Draggable = true

ScrollingFrame.Parent = Frame
ScrollingFrame.Active = true
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
ScrollingFrame.BorderColor3 = Color3.fromRGB(24, 24, 24)
ScrollingFrame.Position = UDim2.new(0, 0, 0.310122341, 0)
